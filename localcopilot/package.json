{
    "name":  "codemate",
    "displayName":  "CodeMate",
    "description":  "",
    "version":  "0.0.1",
    "engines":  {
                    "vscode":  "^1.109.0"
                },
    "categories":  [
                       "Other"
                   ],
    "activationEvents":  [
                             "onView:codemate.agentView"
                         ],
    "main":  "./out/extension.js",
    "contributes":  {
                        "commands":  [
                                         {
                                             "command":  "codemate.helloWorld",
                                             "title":  "Hello World"
                                         },
                                         {
                                             "command":  "codemate.githubManager",
                                             "title":  "CodeMate: GitHub Manager (Test)"
                                         }
                                     ],
                        "configuration":  {
                                              "title":  "CodeMate",
                                              "properties":  {
                                                                 "codemate.inlineModel":  {
                                                                                                  "type":  "string",
                                                                                                  "default":  "",
                                                                                                  "description":  "Ollama model for inline completions. Leave empty to use the first available model."
                                                                                              },
                                                                 "codemate.groqApiKey":  {
                                                                                                  "type":  "string",
                                                                                                  "default":  "",
                                                                                                  "description":  "Groq API key for model list and requests."
                                                                                              },
                                                                 "codemate.pythonPath":  {
                                                                                                  "type":  "string",
                                                                                                  "default":  "",
                                                                                                  "description":  "Optional Python command for MCP/agents (e.g. \"py -3\" or full path). If empty, uses py -3 on Windows, python3 on Unix."
                                                                                              }
                                                             }
                                          },
                        "viewsContainers":  {
                                                "activitybar":  [
                                                                    {
                                                                        "id":  "codemate",
                                                                        "title":  "CodeMate",
                                                                        "icon":  "media/icon.svg"
                                                                    }
                                                                ]
                                            },
                        "views":  {
                                      "codemate":  [
                                                           {
                                                               "id":  "codemate.agentView",
                                                               "name":  "CodeMate Agent",
                                                               "type":  "webview"
                                                           }
                                                       ]
                                  }
                    },
    "scripts":  {
                    "vscode:prepublish":  "npm run compile",
                    "compile":  "tsc -p ./",
                    "watch":  "tsc -watch -p ./",
                    "pretest":  "npm run compile \u0026\u0026 npm run lint",
                    "lint":  "eslint src",
                    "test":  "vscode-test"
                },
    "devDependencies":  {
                            "@types/vscode":  "^1.109.0",
                            "@types/mocha":  "^10.0.10",
                            "@types/node":  "22.x",
                            "typescript-eslint":  "^8.54.0",
                            "eslint":  "^9.39.2",
                            "typescript":  "^5.9.3",
                            "@vscode/test-cli":  "^0.0.12",
                            "@vscode/test-electron":  "^2.5.2"
                        },
    "license":  "MIT",
    "repository":  {
                       "type":  "git",
                       "url":  "https://github.com/AyushKaithwas978/CodeMate"
                   }
}

